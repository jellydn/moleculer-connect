{
  "name": "moleculer-connect",
  "version": "0.2.2",
  "description": "Moleculer connect from CLI",
  "keywords": ["cli", "moleculer", "microservices"],
  "homepage": "https://github.com/jellydn/moleculer-connect#readme",
  "bugs": {
    "url": "https://github.com/jellydn/moleculer-connect/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jellydn/moleculer-connect.git"
  },
  "license": "MIT",
  "author": "Dung Huynh Duc<dung@productsway.com>",
  "type": "module",
  "bin": {
    "moleculer-connect": "dist/cli.js"
  },
  "files": ["dist", "src"],
  "scripts": {
    "build": "tsup",
    "dev": "npx tsx ./src/cli.ts",
    "format": "biome format **/*.ts",
    "check": "biome check --apply .",
    "install-husky": "npx husky install",
    "lint": "biome lint --apply **/*.ts"
  },
  "nano-staged": {
    "*.{js,ts}": "biome format",
    "*.js": "biome lint"
  },
  "dependencies": {
    "glob": "^10.3.12",
    "lodash": "^4.17.21",
    "moleculer": "^0.14.33",
    "nats": "^2.21.0",
    "pino": "^8.20.0",
    "rimraf": "^5.0.5",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.4",
    "@types/node": "^20.12.5",
    "@types/yargs": "^17.0.32",
    "husky": "^9.0.11",
    "nano-staged": "^0.8.0",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.4"
  },
  "peerDependencies": {
    "moleculer-repl": "0.6.6"
  },
  "packageManager": "pnpm@8.15.6"
}
